{
  "name": "slgl",
  "version": "0.0.4",
  "author": "Jeff Schofield <jeff@jeffschofield.com>",
  "keywords": [
    "webgl",
    "glsl",
    "shaders"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "tsup"
  },
  "gitHooks": {
    "commit-msg": "esno scripts/verifyCommit.ts"
  },
  "devDependencies": {
    "@types/node": "^16.11.39",
    "chalk": "^5.0.1",
    "esno": "^0.16.3",
    "tsup": "^6.1.2",
    "typescript": "^4.7.3",
    "yorkie": "^2.0.0"
  }
}
